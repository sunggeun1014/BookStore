<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ezen.bookstore.mobile.mapper.DeliveryMapper">
	<select id="getRequestList" resultType="deliveryDTO">
		SELECT 
			request_num,
			request_date,
			manager_id
		FROM
			delivery_request
		WHERE
			request_end_date IS NULL
	
	</select>
</mapper>